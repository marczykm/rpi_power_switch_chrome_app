{
  "manifest_version": 2,

  "name": "Power management",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "permissions": [
    "webview"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "android-icon-16x16.png", "128": "android-icon-128x128.png" }
}